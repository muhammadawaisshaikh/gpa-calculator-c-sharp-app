<Window x:Class="gpa.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="550" Width="900" Icon="outer.png">
    <Grid Background="#FF272222">
        <Label Content="" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="114" Width="136">
            <Label.Background>
                <ImageBrush ImageSource="inner.png"/>
            </Label.Background>
        </Label>
        <Frame x:Name="mainframe" Content="" HorizontalAlignment="Left" Height="484" Margin="223,25,0,0" VerticalAlignment="Top" Width="659" NavigationUIVisibility="Hidden"/>
        <Label x:Name="timelabel" Content="" HorizontalAlignment="Left" Margin="20,425,0,0" VerticalAlignment="Top" Height="56" Width="178" FontFamily="Century Gothic" FontSize="20" Foreground="#FF2990AE" FontWeight="Bold" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.347"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>

    </Grid>
</Window>
